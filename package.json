{
  "name": "vymalo-food-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "build": "tsc -b && vite build",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "preview": "vite preview",
    "generate-pwa-assets": "pwa-assets-generator && mkdir -p public/images && mv public/*.{ico,png} public/images",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "prepare": "husky",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --ui -w",
    "build-storybook": "storybook build",
    "vercel-build": "yarn codegen && yarn generate-pwa-assets && yarn build",
    "postinstall": "husky"
  },
  "dependencies": {
    "@medusajs/js-sdk": "^2.1.3",
